    "@vue/test-utils": "^2.0.0",
    "@vue/tsconfig": "^0.1.3",
    "eslint": "^8.5.0",
    "eslint-plugin-vue": "^8.2.0",
    "jsdom": "^20.0.0",
    "less": "^4.1.3",
    "naive-ui": "^2.30.6",
    "prettier": "^2.7.1",
    "typescript": "^4.7.4",
    "unocss": "^0.41.1",
    "unplugin-icons": "^0.14.6",
    "unplugin-vue-components": "^0.20.1",
    "vite": "^2.9.12",
    "vite-plugin-html": "^3.2.0",
    "vite-plugin-mock": "^2.9.6",
    "vitest": "^0.16.0",
